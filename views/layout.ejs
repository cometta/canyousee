<style>

@media only screen and (max-width:299px) {
	.smalls {
		display:none;
	}

}

@media only screen and (max-width:600px) {
   .compOnly {
   	   display:none !important;
   }

}

@media only screen and (min-width:601px) {
 .compOnly {
   	   display:display-inline;
   }
}

.ui.menu .item {
 line-height: normal !important;
